<h1><%= @address.id ? 'Edit Address' : 'Create Address' %></h1>
<hr />
<%= form_for @address do |f| %>
  <%= f.label :address %>
  <%= f.text_field :name, required: true, autofocus: true %>
  <%= f.label :city %>
  <%= f.text_field :name, required: true, autofocus: true %>
  <%= f.label :state %>
  <%= f.text_field :name, required: true, autofocus: true %>
  <%= f.label :zip %>
  <%= f.text_field :integer, required: true, autofocus: true %>
  <%= f.submit %>
<% end %>

